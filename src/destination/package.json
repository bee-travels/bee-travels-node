{
  "name": "destination",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test-suite": "babel-node test/run",
    "test": "npm-run-all test-suite",
    "start": "NODE_ENV=production node -r esm ./src/bin/www",
    "dev": "nodemon",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .js ./src",
    "format": "prettier --write 'src/**/*.+(js|json)'"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "express": "~4.16.0",
    "fast-csv": "^3.4.0",
    "http-errors": "~1.6.2",
    "swagger-ui-express": "^4.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "acorn": "^7.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "mocha": "^6.2.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.+(js|json)": "prettier --write"
  }
}
